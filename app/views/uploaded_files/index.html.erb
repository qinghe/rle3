<h1>Listing template_files</h1>

<table>
  <tr>
    <th>Layout</th>
    <th>TemplateTheme</th>
    <th>File name</th>
    <th>Original file name</th>
    <th>File size</th>
    <th>Width</th>
    <th>Height</th>
  </tr>

<% @template_files.each do |uploaded_image| %>
  <tr>
    <td><%=h uploaded_image.layout_id %></td>
    <td><%=h uploaded_image.theme_id %></td>
    <td><%=h uploaded_image.file_name %></td>
    <td><%=h uploaded_image.original_file_name %></td>
    <td><%=h uploaded_image.file_size %></td>
    <td><%=h uploaded_image.width %></td>
    <td><%=h uploaded_image.height %></td>
    <td><%= link_to 'Show', uploaded_image %></td>
    <td><%= link_to 'Edit', edit_uploaded_image_path(uploaded_image) %></td>
    <td><%= link_to 'Destroy', uploaded_image, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New uploaded_image', new_uploaded_image_path %>