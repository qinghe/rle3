<h1>Listing menus</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Perma name</th>
    <th>Root</th>
    <th>Parent</th>
    <th>Lft</th>
    <th>Rgt</th>
    <th>Is enabled</th>
  </tr>

<% @menus.each do |menu| %>
  <tr>
    <td><%=h menu.title %></td>
    <td><%=h menu.perma_name %></td>
    <td><%=h menu.root_id %></td>
    <td><%=h menu.parent_id %></td>
    <td><%=h menu.lft %></td>
    <td><%=h menu.rgt %></td>
    <td><%=h menu.is_enabled %></td>
    <td><%= link_to 'Show', menu %></td>
    <td><%= link_to 'Edit', edit_menu_path(menu) %></td>
    <td><%= link_to 'Destroy', menu, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New menu', new_menu_path %>
